/**index.wxss**/
@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.main{
	width:100%;
	height:100vh;
	position:relative;
	overflow:hidden;
}
.main image{
	width:100%;
	height:100%;
}
.main_cont{
	width:100%;
	height:100%;
	position: absolute;
	left:0;
	top:0;
}
.contain{
	top:360rpx;
	width:100%;
	height:666rpx;
}
.entrance{
	width:81.33%;
	margin:0 auto;
	height:100%;
	flex-wrap: wrap;
}
.entrance_item{
	width:280rpx;
	height:280rpx;
	border-radius:30rpx;
	background:rgba(255,255,255,0.8);
	text-align:center;
}
.entrance_item_in{
	width:100%;
	text-align:center
}
.entrance_item_img image{
	width:100rpx;
	height:100rpx;
}